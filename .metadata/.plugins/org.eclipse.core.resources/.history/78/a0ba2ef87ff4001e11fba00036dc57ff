spring:
  application:
    name: user-service

   
  datasource:
    url: jdbc:mysql://localhost:3306/microservices?ssl=true
    username: root
    password: system
    driver-class-name: com.mysql.cj.jdbc.Driver
  jpa:
    show_sql: true 
    hibernate:
      ddl-auto: update
      
    properties:
      hibernate:
        dialect: org.hibernate.dialect.MySQL8Dialect
       
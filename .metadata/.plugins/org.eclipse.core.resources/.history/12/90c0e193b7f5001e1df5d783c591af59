server:
  port: 8084

spring:
  application:
    name: Api-Gateway
    
    

  cloud:
    gateway:
      routes:
      - id: USER-SERVICE
        uri: LB://USER-SERVICE
        predicates:
          Path=/user/**
    
    
eureka:
  client:
    register-with-eureka: true
    fetch-registry: true
    service-url:
      defaultZone: http://localhost:8761/eureka     